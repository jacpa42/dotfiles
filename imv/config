[options]
overlay_font = JetBrainsMonoNerdFont:18
overlay = true
overlay_background_alpha = 00
background=1e222b

[binds]
w = exec $XDG_CONFIG_HOME/hypr/scripts/cyclewall.sh -s $imv_current_file

<Meta+l> = next
<Meta+h> = prev

<Meta+k> = zoom 1
<Meta+j> = zoom -1

<Ctrl+r> = rotate by 90
<Ctrl+Shift+r> = rotate by -90
