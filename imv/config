[options]
overlay_font = JetBrainsMonoNerdFont:18
overlay = true
overlay_background_alpha = 00
background=121212

[binds]
w = exec $XDG_CONFIG_HOME/hypr/cyclewall.sh -d $imv_current_file

<Meta+l> = next
<Meta+h> = prev

<Meta+k> = zoom 1
<Meta+j> = zoom -1

<Ctrl+r> = rotate 90
<Ctrl+Shift+r> = rotate -90
